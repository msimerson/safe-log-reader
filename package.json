{
  "name": "safe-log-reader",
  "description": "Safe Log Reader",
  "version": "0.9.1",
  "private": false,
  "homepage": "https://github.com/DoubleCheck/safe-log-reader",
  "bugs": {
    "url": "https://github.com/DoubleCheck/safe-log-reader/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/DoubleCheck/safe-log-reader.git"
  },
  "dependencies": {
  },
  "devDependencies": {
    "blanket": "^1.1.7",
    "coveralls": "^2.11.2",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "0.0.2"
  },
  "config": {
    "blanket": {
      "pattern": [ "index.js", "lib" ],
      "data-cover-never": [ "node_modules", "test" ]
    }
  },
  "scripts": {
    "test": "mocha",
    "coveralls": "COVERAGE=1 ./node_modules/.bin/mocha --require blanket --reporter mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js"
  },
  "optionalDependencies": {}
}
